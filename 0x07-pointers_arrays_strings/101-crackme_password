#include <stdio.h>
#include <stdlib.h>
#include <string.h>

/**
*main- main function
*Return: return 0
*/

int main()
{
	char password[] = "your_password";

	char file_path[] = "password.txt";

	FILE *file = fopen(file_path, "w");
	if (file == NULL)
	{
		printf("Failed to create the file.\n");
		return 1;
	}
	fwrite(password, sizeof(char), strlen(password), file);
	fclose(file);
	printf("Password file created successfully.\n");
	return 0;
}

